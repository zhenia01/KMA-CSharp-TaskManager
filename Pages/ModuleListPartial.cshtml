@model (string ProcessInfo, System.Diagnostics.ProcessModuleCollection Modules)

<div class="modal fade" id="module-list" tabindex="-1" role="dialog" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title">Module List - @Model.ProcessInfo</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <table class="table table-hover table-sm">
          <thead>
          <tr>
            <th scope="col" class="align-middle">#</th>
            <th scope="col">Name</th>
            <th scope="col">Path</th>
          </tr>
          </thead>
          <tbody>
          @for (int i = 0; i < Model.Modules.Count; i++)
          {
            var module = Model.Modules[i];
              <tr>
                <th scope="row">@(i+1)</th>
                <td class="long-wrap" title="@module.ModuleName">@module.ModuleName</td>
                <td class="long-wrap" title="@module.FileName">@module.FileName</td>
              </tr>
            }
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>